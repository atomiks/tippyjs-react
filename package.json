{
  "name": "@tippy.js/react",
  "version": "1.1.1",
  "description": "React component for Tippy.js",
  "main": "dist/Tippy.js",
  "module": "dist/esm/Tippy.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "parcel demo/index.html -d .devserver --no-cache --open",
    "build": "node rollup.build.js",
    "test": "jest --coverage",
    "lint": "eslint \"{src,test}/**/*.js\"",
    "format": "prettier --write \"{src,test}/**/*.{js,json,css,md}\""
  },
  "author": "atomiks",
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>test/setup.js",
    "coveragePathIgnorePatterns": [
      "test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "{src,test}/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "tippy.js": "^3.2.0"
  },
  "peerDependencies": {
    "react": ">=16.2",
    "react-dom": ">=16.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^23.5.0",
    "lint-staged": "^8.1.0",
    "parcel": "^1.10.3",
    "prettier": "^1.16.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-testing-library": "^5.2.3",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-babel-minify": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atomiks/tippy.js-react.git"
  },
  "bugs": {
    "url": "https://github.com/atomiks/tippy.js-react/issues"
  },
  "homepage": "https://github.com/atomiks/tippy.js-react#readme"
}
